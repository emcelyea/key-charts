{
  "name": "EvenFlowCharts",
  "title": "EvenFlowCharts",
  "version": "0.0.1",
  "homepage": "",
  "author": "Derrick McElyea",
  "license": {
    "type": "MIT"
  },
  "description": "draws gantt charts",
  "scripts": {
    "start": "node server.js",
    "debug": "node --inspect server.js",
   
    "build-gantt": "gulp build-gantt",
    "develop-gantt": "gulp build-gantt-watch",
    "test-gantt": "./node_modules/gulp/bin/gulp.js lint-gantt && ./node_modules/karma/bin/karma start ./tests/karma/gantt/karma.conf.js",
   
    "build-flow":"gulp build-flow",
    "develop-flow":"gulp build-flow-watch",  
    "test-flow": "gulp build-flow && ./node_modules/gulp/bin/gulp.js lint-flow && ./node_modules/karma/bin/karma start ./tests/karma/flow/karma.conf.js",
    
    "lint-server":"gulp lint-server",
    "es6":"gulp es6-watch"
  },
  "devDependencies": {
    "babel-preset-es2016": "^6.22.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.0.4",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jshint": "^2.9.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0"
  },
  "dependencies": {
    "body-parser": "1.16.0",
    "express": "4.14.0",
    "mongoose": "4.7.7",
    "morgan": "^1.7.0"
  }
}
